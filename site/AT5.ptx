            <p original-id="AT5-s3a10s1o3l1p1" internal-id="p-AT5-c-j-a-c-a-a">
            <m original-id="AT5-s3a10s1o3l1p1m1" internal-id="m-AT5-c-j-a-c-a-a-a">(0, 0)</m>
            </p>
        </li>
          <li original-id="AT5-s3a10s1o3l2" internal-id="li-AT5-c-j-a-c-b">
            <p original-id="AT5-s3a10s1o3l2p1" internal-id="p-AT5-c-j-a-c-b-a">
